{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_APP_NAME": "ToU AI Assistant",
    "VITE_APP_VERSION": "3.0.0"
  },
  "build": {
    "env": {
      "VITE_API_URL": "@vite_api_url"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://tou-ai-assistant.onrender.com/api/$1"
      "destination": "https://aiassistant-d9df.onrender.com/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
